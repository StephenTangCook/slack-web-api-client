{
  "name": "slack-web-api-client",
  "version": "0.8.0",
  "description": "Streamlined Slack Web API client for TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:clean": "rm -rf ./dist && tsc",
    "format": "npx prettier --write src/ test/",
